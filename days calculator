from datetime import date

print("Please enter your start date information: ")
start_year = int(input("Year: ")) 
start_month = int(input("Month: "))
start_day = int(input("Day: "))

start_date = date(start_year, start_month, start_day)

print("Please enter your end date information: ")
end_year = int(input("Year: "))
end_month = int(input("Month: "))
end_day = int(input("Day: "))

end_date = date(end_year, end_month, end_day)

number_of_days = end_date - start_date

print("The number of days between these two dates is: ", number_of_days.days)